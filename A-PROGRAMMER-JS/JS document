-----------------------------------------------

var n1 = 1;
var n2 = 3;
var n3 = 1;
var n4 = 'abcd';

if (n3 > n2) {
	console.log('GREATER');
}else if (n3 == n2) {
	console.log('EQUAL');
}else if (n3 < n2) {
	console.log('LESS');
}else{
	console.log('NONE');
}
-----------------------------------------------

var n1 = 1;
var n2 = 2;
var n3 = 3;
var n4 = 'abcd';
var x = n3 - n1;
switch (x) {
	case 3:
		console.log('this is 3');
		break;
	case 2:
		console.log("this is 2");
		break;
	default:
		console.log('this is default');
}
